<GridPane hgap="20.0" vgap="20.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	alignment="TOP_CENTER" fx:id="pane">

	<Text text="Anzahl:" GridPane.rowIndex="0" />

	<Text text="Durchschnitt:" GridPane.rowIndex="1" />

	<Text text="Minimum:" GridPane.rowIndex="2" />

	<Text text="Maximum:" GridPane.rowIndex="3" />

	<Text text="Standardabweichung:" GridPane.rowIndex="4" />

	<Text text="95%-Intervall:" GridPane.rowIndex="5" />
	
	<Text text="Visualisierung:" GridPane.rowIndex="6" />
