<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="20.0" vgap="20.0" fx:controller="de.diptalyzer.controller.MainFrameController"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	alignment="TOP_CENTER">
	<HBox alignment="CENTER" spacing="25.0" GridPane.columnSpan="2">
		<Text text="Glyphe:" />
		<MenuButton fx:id="selGlyph" alignment="CENTER_LEFT"
			mnemonicParsing="false" text="wählen" minWidth="100" />
		<Text text="Filter:" />
		<MenuButton fx:id="selFilter" alignment="CENTER_LEFT"
			mnemonicParsing="false" text="wählen" minWidth="150" disable="true" />
	</HBox>
	<Pane fx:id="leftPane" GridPane.rowIndex="1" minWidth="100"
		minHeight="100" />
	<Pane fx:id="rightPane" GridPane.columnIndex="1"
		GridPane.rowIndex="1" minWidth="100" minHeight="100" />
	<padding>
		<Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
	</padding>
</GridPane>
